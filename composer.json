{
    "name": "masteryuriy/laravel-edit-trans",
    "type": "library",
    "description": "Manage Laravel Translations",
    "keywords": [
        "masteryuriy",
        "laravel-edit-trans",
        "manage localization",
        "generate localization"
    ],
    "homepage": "https://github.com/MasterYuri/Laravel-EditTrans",
    "license": "MIT",
    "authors": [
        {
            "name": "Yuriy Guzenko",
            "email": "yuriyg86@gmail.com",
            "homepage": "https://github.com/MasterYuri",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "intervention/image": "2.*",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "MasterYuri\\EditTrans\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MasterYuri\\EditTrans\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
